{
  "title": "Docker Commands",
  "language": "en",
  "cheats": [
    {
      "description": "Run a container",
      "code": "docker run image",
      "example": "# Starts new container from image"
    },
    {
      "description": "List containers",
      "code": "docker ps",
      "example": "# Lists running containers"
    },
    {
      "description": "Stop a container",
      "code": "docker stop container",
      "example": "# Docker operation"
    },
    {
      "description": "Remove a container",
      "code": "docker rm container",
      "example": "# Docker operation"
    },
    {
      "description": "List images",
      "code": "docker images",
      "example": "# Docker operation"
    },
    {
      "description": "Remove image",
      "code": "docker rmi image",
      "example": "# Docker operation"
    },
    {
      "description": "Build image",
      "code": "docker build -t name .",
      "example": "# Creates image from Dockerfile"
    },
    {
      "description": "Show logs",
      "code": "docker logs container",
      "example": "# Docker operation"
    }
  ]
}