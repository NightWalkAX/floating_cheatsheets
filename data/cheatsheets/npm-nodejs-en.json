{
  "title": "NPM & Node.js",
  "language": "en",
  "tags": ["npm", "nodejs", "javascript", "package-manager"],
  "items": [
    {
      "code": "npm --version",
      "description": "Check npm version",
      "example": "npm --version | npm -v"
    },
    {
      "code": "node --version",
      "description": "Check Node.js version",
      "example": "node --version | node -v"
    },
    {
      "code": "npm init",
      "description": "Create a package.json file",
      "example": "npm init | npm init -y (with default values)"
    },
    {
      "code": "npm install",
      "description": "Install dependencies",
      "example": "npm install | npm install package-name | npm install -g package-name"
    },
    {
      "code": "npm install --save-dev",
      "description": "Install development dependencies",
      "example": "npm install --save-dev eslint | npm install -D prettier"
    },
    {
      "code": "npm uninstall",
      "description": "Uninstall packages",
      "example": "npm uninstall package-name | npm uninstall -g package-name"
    },
    {
      "code": "npm list",
      "description": "List installed packages",
      "example": "npm list | npm list -g | npm list --depth=0"
    },
    {
      "code": "npm outdated",
      "description": "Check for outdated packages",
      "example": "npm outdated | npm outdated -g"
    },
    {
      "code": "npm update",
      "description": "Update packages",
      "example": "npm update | npm update package-name"
    },
    {
      "code": "npm run",
      "description": "Run scripts from package.json",
      "example": "npm run start | npm run build | npm run test"
    },
    {
      "code": "npm start",
      "description": "Run the start script",
      "example": "npm start (equivalent to npm run start)"
    },
    {
      "code": "npm test",
      "description": "Run tests",
      "example": "npm test | npm t"
    },
    {
      "code": "npm run build",
      "description": "Build the project",
      "example": "npm run build"
    },
    {
      "code": "npm publish",
      "description": "Publish a package",
      "example": "npm publish | npm publish --tag beta"
    },
    {
      "code": "npm search",
      "description": "Search for packages",
      "example": "npm search express | npm search react router"
    },
    {
      "code": "npm info",
      "description": "Get package information",
      "example": "npm info express | npm info react versions --json"
    },
    {
      "code": "npx",
      "description": "Execute packages without installing",
      "example": "npx create-react-app my-app | npx eslint file.js"
    },
    {
      "code": "npm cache clean",
      "description": "Clear the npm cache",
      "example": "npm cache clean --force"
    },
    {
      "code": "npm audit",
      "description": "Audit for vulnerabilities",
      "example": "npm audit | npm audit fix"
    },
    {
      "code": "node archivo.js",
      "description": "Execute a JavaScript file",
      "example": "node app.js | node --inspect app.js (debug)"
    },
    {
      "code": "npm config",
      "description": "Manage npm configuration",
      "example": "npm config list | npm config set registry https://registry.npmjs.org/"
    },
    {
      "code": "package.json scripts",
      "description": "Custom scripts",
      "example": "\"scripts\": { \"dev\": \"node server.js\", \"build\": \"webpack\" }"
    }
  ],
  "created": "2025-09-24",
  "updated": "2025-10-01"
}