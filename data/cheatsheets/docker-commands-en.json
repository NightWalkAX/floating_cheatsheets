{
  "title": "Docker Commands",
  "language": "en",
  "tags": ["docker", "containers", "devops"],
  "items": [
    {
      "code": "docker --version",
      "description": "Show Docker version information",
      "example": "docker --version"
    },
    {
      "code": "docker pull",
      "description": "Download an image from a registry",
      "example": "docker pull ubuntu:20.04"
    },
    {
      "code": "docker images",
      "description": "List downloaded images",
      "example": "docker images -a"
    },
    {
      "code": "docker run",
      "description": "Run a command in a new container",
      "example": "docker run -it -d -p 8080:80 --name webhost nginx"
    },
    {
      "code": "docker ps",
      "description": "List running containers",
      "example": "docker ps -a"
    },
    {
      "code": "docker stop",
      "description": "Stop one or more running containers",
      "example": "docker stop webhost"
    },
    {
      "code": "docker start",
      "description": "Start one or more stopped containers",
      "example": "docker start webhost"
    },
    {
      "code": "docker restart",
      "description": "Restart a container",
      "example": "docker restart webhost"
    },
    {
      "code": "docker rm",
      "description": "Remove one or more containers",
      "example": "docker rm webhost"
    },
    {
      "code": "docker rmi",
      "description": "Remove one or more images",
      "example": "docker rmi nginx"
    },
    {
      "code": "docker exec",
      "description": "Run a command in a running container",
      "example": "docker exec -it webhost bash"
    },
    {
      "code": "docker logs",
      "description": "Fetch the logs of a container",
      "example": "docker logs -f webhost"
    },
    {
      "code": "docker build",
      "description": "Build an image from a Dockerfile",
      "example": "docker build -t my-app:1.0 ."
    },
    {
      "code": "docker push",
      "description": "Push an image or a repository to a registry",
      "example": "docker push my-username/my-app:1.0"
    },
    {
      "code": "docker tag",
      "description": "Create a tag TARGET_IMAGE that refers to SOURCE_IMAGE",
      "example": "docker tag my-app:1.0 my-username/my-app:latest"
    },
    {
      "code": "docker network",
      "description": "Manage networks",
      "example": "docker network create my-net"
    },
    {
      "code": "docker volume",
      "description": "Manage volumes",
      "example": "docker volume create my-vol"
    },
    {
      "code": "docker-compose up",
      "description": "Create and start containers from a docker-compose.yml file",
      "example": "docker-compose up -d"
    },
    {
      "code": "docker-compose down",
      "description": "Stop and remove containers, networks, images, and volumes",
      "example": "docker-compose down -v"
    },
    {
      "code": "docker system prune",
      "description": "Remove all unused containers, networks, images (both dangling and unreferenced)",
      "example": "docker system prune -a -f --volumes"
    }
  ],
  "created": "2025-09-24",
  "updated": "2025-10-01"
}